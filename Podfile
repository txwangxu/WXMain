project 'WXMain.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '7.0'

inhibit_all_warnings!

target 'WXMain' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for WXMain
  
  pod 'Masonry'
  
  pod 'AFNetworking'
  
  pod 'MJRefresh'
  
  pod 'MJExtension'
  
  pod 'SDWebImage'
  
  pod 'FMDB'
  
#   pod 'ReactiveObjC'
  
  pod 'Reveal-iOS-SDK' ,:configurations => ['Debug']
  

  target 'WXMainTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WXMainUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
